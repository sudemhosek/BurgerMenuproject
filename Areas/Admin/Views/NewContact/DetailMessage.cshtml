@model project2BurgerMenu.entityes.contact
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <p class="card-description"></p>
            <div class="col-md-12 grid-margin-lg-0 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <div class="form-group row">
                            <div class="col">
                                <label>Gönderen</label>
                                <div id="the-basics">
                                    <input class="typeahead" type="text" value="@Model.name" readonly>
                                </div>
                            </div>
                            <div class="col">
                                <label>Email</label>
                                <div id="bloodhound">
                                    <input class="typeahead" type="text" value="@Model.email" readonly>
                                </div>
                            </div>
                            <div class="col-12">
                                <label>Mesaj</label>
                                <div id="bloodhound">
                                    <textarea class="form-control" rows="5" readonly>@Model.detail</textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
